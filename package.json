{
  "name": "poll",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "run-s db:generate && next dev",
    "build": "run-s db:generate && next build && cp -r .next/static .next/standalone/.next/static",
    "run-prod": "node --env-file='.env.local' .next/standalone/server.js",
    "db:generate": "prisma generate --no-hints",
    "db:migrate": "DB_URL='file:dev.db' prisma migrate deploy",
    "studio": "prisma studio",
    "test": "run-s db:generate && run-p -cl test:*",
    "test:eslint": "eslint --color '{src,test}/**/*.{js,ts,tsx}'",
    "test:prettier": "prettier -l '{src,test}/**/*.{css,js,ts,tsx}'",
    "test:ts": "tsc --pretty --noEmit",
    "prettier": "prettier --write '{src,test}/**/*.{css,js,ts,tsx}'"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
    "@floating-ui/react": "^0.27.3",
    "@headlessui/react": "^2.2.0",
    "@prisma/client": "^6.2.1",
    "d3": "^7.9.0",
    "date-fns": "^4.1.0",
    "discord.js": "^14.17.3",
    "ky": "^1.7.4",
    "next-auth": "^5.0.0-beta.25",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "znv": "^0.4.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.6",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-config-lostfictions": "7.0.0-beta.0",
    "next": "^15.1.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "prisma": "^6.2.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  },
  "browserslist": {
    "production": [
      "> 3%",
